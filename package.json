  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MilesBHuff/Pokedex.git"
  },
  "private": true,
  "directories": {
    "bin": "./dist",
    "lib": "./src"
  },
  "type": "module",
  "scripts": {
    "start": "npm run dev",
