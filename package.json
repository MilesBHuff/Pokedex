{
  "name": "pokedex",
  "version": "1.0.0",
  "license": "LAGPL-3.0-or-later",
  "description": "A web application that displays data on Pokémon®. Not affiliated with Nintendo®.",
  "keywords": [
    "Pokédex",
    "Pokémon",
    "demo",
    "React",
    "Vite"
  ],
  "author": {
    "name": "Miles B Huff",
    "email": "MilesBHuff@NoReply.GitHub.com"
  },
  "homepage": "https://github.com/MilesBHuff/Pokedex#readme",
  "bugs": {
    "url": "https://github.com/MilesBHuff/Pokedex/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MilesBHuff/Pokedex.git"
  },
  "private": true,
  "directories": {
    "bin": "./dist",
    "lib": "./src"
  },
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "test": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "check": "tsc",
    "build": "tsc && vite build",
    "preview": "vite preview",    
    "relock": "rm -rf node_modules && rm -f npm-shrinkwrap.json && npm install && npm shrinkwrap"
  },
  "engines": {
    "node": "^20",
    "npm": "^10"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^8"
  },
  "devDependencies": {
    "@testing-library/dom": "^9",
    "@testing-library/jest-dom": "^5",
    "@testing-library/react": "^14",
    "@testing-library/user-event": "^14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6",
    "@typescript-eslint/parser": "^6",
    "@types/testing-library__jest-dom": "^5",
    "@vitejs/plugin-react": "^4",
    "eslint": "^8",
    "eslint-plugin-react-hooks": "^4",
    "eslint-plugin-react-refresh": "~0.4",
    "jsdom": "^21",
    "typescript": "~5.0",
    "vite": "^4",
    "vitest": "~0.30"
  }
}
